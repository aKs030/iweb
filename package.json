{
  "name": "aks-website",
  "version": "1.0.0",
  "description": "Portfolio Website mit Cloudflare AI Search",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint && npm run format",
    "clean": "rm -rf .wrangler node_modules/.cache",
    "push": "git add -A && git commit -m 'Update' && git push origin main"
  },
  "dependencies": {
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.2",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3"
  }
}
