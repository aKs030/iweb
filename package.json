{
  "name": "iweb",
  "version": "1.0.0",
  "description": "Portfolio website of Abdulkerim Sesli - Web Developer & Photographer in Berlin",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:legacy": "node scripts/dev-server.js",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{js,css,json,md,html}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,css,json,md,html}\" --ignore-path .prettierignore",
    "lint": "eslint \"**/*.js\" --fix",
    "lint:check": "eslint \"**/*.js\"",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint && npm run format",
    "analyze": "vite-bundle-visualizer",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "vite": "^6.0.5",
    "vite-bundle-visualizer": "^1.2.1",
    "terser": "^5.37.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aKs030/iweb.git"
  },
  "keywords": [
    "portfolio",
    "webdev",
    "photography",
    "berlin"
  ],
  "author": "Abdulkerim Sesli",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "bugs": {
    "url": "https://github.com/aKs030/iweb/issues"
  },
  "homepage": "https://github.com/aKs030/iweb#readme",
  "dependencies": {
    "dompurify": "^3.3.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "three": "^0.182.0"
  }
}
