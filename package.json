{
  "name": "aks-website",
  "version": "1.0.0",
  "description": "Portfolio Website mit Cloudflare AI Search (Vanilla JS)",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.22.0"
  },
  "scripts": {
    "dev": "wrangler pages dev . --port 8080",
    "dev:sim": "node server.js",
    "smoke": "node scripts/smoke-test.mjs",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint:check && npm run format:check",
    "check:all": "npm run check && npm run smoke",
    "fix": "npm run lint && npm run format",
    "clean": "rm -rf .wrangler node_modules/.cache",
    "prepare": "husky"
  },
  "dependencies": {
    "three": "^0.171.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "eslint": "^9.39.3",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.8.1",
    "wrangler": "^4.67.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,html,json}": [
      "prettier --write"
    ]
  }
}
