{
  "name": "aks-website",
  "version": "1.0.0",
  "description": "Portfolio Website mit Cloudflare AI Search",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev dist",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint && npm run format",
    "knip": "knip",
    "quality": "npm run check && npm run knip",
    "css:check": "echo 'Checking CSS stats...'",
    "clean": "rm -rf dist .vite .wrangler",
    "deploy": "wrangler pages deploy dist"
  },
  "dependencies": {
    "dompurify": "^3.2.0",
    "htm": "^3.1.1",
    "marked": "^17.0.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.14.0",
    "fast-glob": "^3.3.3",
    "globals": "^15.12.0",
    "html-validate": "^8.20.1",
    "jscpd": "^3.5.10",
    "knip": "^5.36.0",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.10",
    "vite-plugin-static-copy": "^3.2.0",
    "wrangler": "^3.114.17"
  }
}
