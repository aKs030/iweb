{
  "name": "aks-website",
  "version": "1.0.0",
  "description": "Portfolio Website mit Cloudflare AI Search",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node server.js",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "css:check": "echo 'Skipping CSS check'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint && npm run format",
    "knip": "knip",
    "quality": "npm run check && npm run knip",
    "perf:budget": "echo 'Performance budget check skipped'",
    "size:check": "npm run build && du -sh dist && find dist -name '*.js' -exec du -h {} \\; | sort -h",
    "clean": "rm -rf dist .vite .wrangler node_modules/.cache",
    "deploy": "wrangler pages deploy dist",
    "audit:fix": "npm audit fix",
    "update": "npm update && npm audit fix"
  },
  "dependencies": {
    "lucide-react": "^0.564.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.2",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "knip": "^5.36.0",
    "madge": "^8.0.0",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.46.0",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "wrangler": "^3.114.17"
  }
}
