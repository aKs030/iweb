{
  "name": "aks-website",
  "version": "1.0.0",
  "description": "Portfolio Website mit Cloudflare AI Search (Vanilla JS)",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "wrangler pages dev . --port 8080",
    "dev:sim": "node server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "check": "npm run lint && npm run format && npm run ai-index:check",
    "fix": "npm run lint:fix && npm run format:write",
    "clean": "rm -rf .wrangler node_modules/.cache",
    "prepare": "husky",
    "docs:check": "node scripts/docs-check.mjs",
    "ai-index:sync": "node scripts/sync-ai-index-apps.mjs",
    "ai-index:check": "node scripts/sync-ai-index-apps.mjs --check"
  },
  "dependencies": {
    "three": "^0.183.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "dompurify": "^3.3.1",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "wrangler": "^4.67.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,html,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
