{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "index.html",
    "pages/**/index.html",
    "content/main.js",
    "pages/**/app.js",
    "pages/**/*-app.js",
    "pages/**/videos.js",
    "pages/**/loader.js",
    "pages/**/*-loader.js",
    "pages/**/*-manager.js",
    "workers/**/index.js",
    "sw.js",
    "content/components/**/*.js",
    "content/core/**/*.js",
    "vite.config.js"
  ],
  "project": ["**/*.js", "**/*.mjs"],
  "ignore": [
    "node_modules/**",
    "dist/**",
    ".git/**",
    "**/*.md",
    "**/*.css",
    "**/*.html",
    "scripts/**",
    "docs/**",
    "reports/**",
    "complexity-report/**"
  ],
  "ignoreDependencies": [
    "dompurify",
    "cssnano",
    "husky",
    "lint-staged",
    "rollup-plugin-visualizer",
    "terser"
  ],
  "ignoreBinaries": ["jscpd", "madge", "es6-plato", "cost-of-modules", "open"],
  "paths": {
    "/content/*": ["content/*"]
  }
}
