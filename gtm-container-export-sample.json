    "tag": [
      {
        "name": "GA4 - Configuration (via gtag.js)",
        "type": "html",
        "parameter": [
          {
            "key": "html",
            "type": "TEMPLATE",
            "value": "<script async src=\"https://www.googletagmanager.com/gtag/js?id={{GA4_MEASUREMENT_ID}}\"></script>\n<script>window.dataLayer = window.dataLayer || [];function gtag(){dataLayer.push(arguments);}gtag('js', new Date());gtag('config', '{{GA4_MEASUREMENT_ID}}', { 'send_page_view': false });</script>"
          },
          {
            "key": "supportDocumentWrite",
            "type": "BOOLEAN",
            "value": "false"
          }
        ],
        "firingTriggerId": ["2147479550"],
        "blockingTriggerId": []
      },
      {
        "name": "Google Ads - Conversion (AW)",
        "type": "html",
        "parameter": [
          {
            "key": "html",
            "type": "TEMPLATE",
            "value": "<script>window.dataLayer = window.dataLayer || [];function gtag(){dataLayer.push(arguments);}gtag('event','conversion',{ 'send_to': '{{AW_CONVERSION_ID}}/{{AW_CONVERSION_LABEL}}' });</script>"
          },
          {
            "key": "supportDocumentWrite",
            "type": "BOOLEAN",
            "value": "false"
          }
        ],
        "firingTriggerId": ["1001"],
        "blockingTriggerId": []
      }
    ],
    "trigger": [
      {
        "triggerId": "2147479550",
        "name": "All Pages",
        "type": "PAGEVIEW",
        "filter": []
      },
      {
        "triggerId": "1001",
        "name": "Consent Granted (event)",
        "type": "CUSTOM_EVENT",
        "filter": [
          {
            "type": "equals",
            "parameter": [
              { "key": "arg0", "type": "TEMPLATE", "value": "event" },
              { "key": "arg1", "type": "TEMPLATE", "value": "consentGranted" }
            ]
          }
        ]
      }
    ],
    "variable": [
