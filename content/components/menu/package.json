{
  "name": "@iweb/menu-system",
  "version": "3.1.0",
  "description": "Modern, modular menu system with enterprise features",
  "type": "module",
  "main": "menu.js",
  "exports": {
    ".": "./menu.js",
    "./modules": "./modules/index.js",
    "./modules/*": "./modules/*.js",
    "./css": "./menu.css"
  },
  "files": [
    "menu.js",
    "menu.css",
    "modules/",
    "README.md"
  ],
  "scripts": {
    "dev": "python3 ../../../scripts/dev-server.js",
    "lint": "eslint modules/ menu.js",
    "format": "prettier --write \"**/*.{js,css,md}\"",
    "docs": "open docs/INDEX.md"
  },
  "keywords": [
    "menu",
    "navigation",
    "es6",
    "modular",
    "accessibility",
    "wcag",
    "performance",
    "theme",
    "state-management"
  ],
  "author": "Abdulkerim Sesli",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "Chrome >= 90",
    "Edge >= 90",
    "Firefox >= 88",
    "Safari >= 14",
    "iOS >= 14"
  ]
}
